nsole.log(error);\r\n        });\r\n    },\r\n    signOut() {\r\n      firebase\r\n        .auth()\r\n        .signOut()\r\n        .then(() => {\r\n          alert('Logged Out');\r\n          this.$router.replace('/');\r\n        })\r\n        .catch((error) => {\r\n          alert(error);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n#info {\r\n  padding: 20px;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components/admin"}]}