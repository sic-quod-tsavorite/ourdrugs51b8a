{"remainingRequest":"D:\\Random Stuff\\MMD\\3rd sem\\Progressive Web Apps\\exam\\ourdrugs\\node_modules\\vue-loader\\lib\\index.js??vue-loader-options!D:\\Random Stuff\\MMD\\3rd sem\\Progressive Web Apps\\exam\\ourdrugs\\src\\views\\Shop.vue?vue&type=style&index=0&lang=scss&","dependencies":[{"path":"D:\\Random Stuff\\MMD\\3rd sem\\Progressive Web Apps\\exam\\ourdrugs\\src\\views\\Shop.vue","mtime":1605209683546},{"path":"D:\\Random Stuff\\MMD\\3rd sem\\Progressive Web Apps\\exam\\ourdrugs\\node_modules\\css-loader\\dist\\cjs.js","mtime":499162500000},{"path":"D:\\Random Stuff\\MMD\\3rd sem\\Progressive Web Apps\\exam\\ourdrugs\\node_modules\\vue-loader\\lib\\loaders\\stylePostLoader.js","mtime":499162500000},{"path":"D:\\Random Stuff\\MMD\\3rd sem\\Progressive Web Apps\\exam\\ourdrugs\\node_modules\\postcss-loader\\src\\index.js","mtime":499162500000},{"path":"D:\\Random Stuff\\MMD\\3rd sem\\Progressive Web Apps\\exam\\ourdrugs\\node_modules\\sass-loader\\dist\\cjs.js","mtime":499162500000},{"path":"D:\\Random Stuff\\MMD\\3rd sem\\Progressive Web Apps\\exam\\ourdrugs\\node_modules\\cache-loader\\dist\\cjs.js","mtime":499162500000},{"path":"D:\\Random Stuff\\MMD\\3rd sem\\Progressive Web Apps\\exam\\ourdrugs\\node_modules\\vue-loader\\lib\\index.js","mtime":499162500000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["Shop.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA+MA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA","file":"Shop.vue","sourceRoot":"src/views","sourcesContent":["<template>\r\n  <v-container id=\"rowCont\">\r\n    <div id=\"drug-table\">\r\n      <v-simple-table>\r\n        <template v-slot:default>\r\n          <thead>\r\n            <th>\r\n              <div class=\"drug-filter\">\r\n                <h2>Filter</h2>\r\n                <div class=\"drug-filter-content\">\r\n                  <a @click=\"filterAll()\">All</a>\r\n                  <a @click=\"filterPills()\">Pills</a>\r\n                  <a @click=\"filterCannabis()\">Cannabis</a>\r\n                  <a @click=\"filterCrystal()\">Crystal</a>\r\n                  <a @click=\"filterPowder()\">Powder</a>\r\n                </div>\r\n              </div>\r\n            </th>\r\n          </thead>\r\n        </template>\r\n      </v-simple-table>\r\n    </div>\r\n    <v-row justify=\"center\" id=\"rowProd\">\r\n      <v-card id=\"products\" v-for=\"item in itemFilter\" :key=\"item.name\">\r\n        <v-img\r\n          id=\"product_img\"\r\n          class=\"white--text\"\r\n          height=\"\"\r\n          :src=\"item.imgURL\"\r\n        >\r\n        </v-img>\r\n        <span>\r\n          {{ item.name }}\r\n        </span>\r\n        <p>{{ item.price }} BTC</p>\r\n        <v-btn color=\"orange accent-2\" @click=\"addToBasket(item)\">BUY</v-btn>\r\n      </v-card>\r\n    </v-row>\r\n    <v-col offset-md=\"1\" md=\"4\" id=\"drugBasket\" v-if=\"user\">\r\n      <h1>\r\n        Basket\r\n      </h1>\r\n      <div id=\"info\" class=\"pa-2\">\r\n        <v-simple-table\r\n          fixed-header\r\n          height=\"300px\"\r\n          id=\"menu-table\"\r\n          v-if=\"basket.length > 0\"\r\n        >\r\n          <template v-slot:default>\r\n            <thead>\r\n              <tr>\r\n                <th class=\"text-left\" style=\"width:30%;\">Quantity</th>\r\n                <th class=\"text-left\" style=\"width:50%;\">Name of item</th>\r\n                <th class=\"text-left\" style=\"width:20%;\">Price</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody>\r\n              <tr v-for=\"item in basket\" :key=\"item.name\">\r\n                <td>\r\n                  <v-icon color=\"orange accent-2\" @click=\"incrQtn(item)\"\r\n                    >add_box</v-icon\r\n                  >\r\n                  {{ item.quantity }}\r\n                  <v-icon color=\"orange accent-2\" @click=\"decrQtn(item)\"\r\n                    >indeterminate_check_box</v-icon\r\n                  >\r\n                </td>\r\n                <td>{{ item.name }}</td>\r\n                <td>{{ item.price }}</td>\r\n              </tr>\r\n            </tbody>\r\n          </template>\r\n        </v-simple-table>\r\n        <v-simple-table v-else>\r\n          <p>\r\n            The basket is empty\r\n          </p>\r\n        </v-simple-table>\r\n        <v-divider></v-divider>\r\n        <v-row class=\"mt-4\" style=\"height: unset;\" id=\"basket_checkout\">\r\n          <v-col>\r\n            <p>Subtotal:</p>\r\n            <p>Delivery:</p>\r\n            <p>Total amount:</p>\r\n          </v-col>\r\n          <v-col class=\"text-right\">\r\n            <p>{{ subTotal }} BTC</p>\r\n            <p>0.0005 BTC</p>\r\n            <p>\r\n              <b>{{ total }} BTC</b>\r\n            </p>\r\n          </v-col>\r\n        </v-row>\r\n        <v-row style=\"height: unset;\">\r\n          <v-spacer></v-spacer>\r\n          <v-btn color=\"orange accent-2\" style=\"margin-right: 10px;\"\r\n            >Checkout</v-btn\r\n          >\r\n        </v-row>\r\n      </div>\r\n    </v-col>\r\n  </v-container>\r\n</template>\r\n\r\n<script>\r\nimport { dbMenuAdd } from '../../firebase';\r\nimport firebase from 'firebase';\r\nimport 'firebase/firestore';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      basket: [],\r\n      menuItems: [],\r\n      filterValue: '',\r\n      user: null,\r\n    };\r\n  },\r\n  created() {\r\n    dbMenuAdd.get().then((querySnapshot) => {\r\n      querySnapshot.forEach((doc) => {\r\n        console.log(doc.id, ' => ', doc.data());\r\n        var menuItemData = doc.data();\r\n        this.menuItems.push({\r\n          id: doc.id,\r\n          name: menuItemData.name,\r\n          description: menuItemData.description,\r\n          price: menuItemData.price,\r\n          filter: menuItemData.filter,\r\n          imgURL: menuItemData.imgURL,\r\n        });\r\n      });\r\n    });\r\n    firebase.auth().onAuthStateChanged((user) => {\r\n      if (user) {\r\n        this.user = user;\r\n      } else {\r\n        this.user = null;\r\n      }\r\n    });\r\n  },\r\n  methods: {\r\n    addToBasket(item) {\r\n      if (this.basket.find((itemInArray) => item.name === itemInArray.name)) {\r\n        item = this.basket.find(\r\n          (itemInArray) => item.name === itemInArray.name\r\n        );\r\n        this.incrQtn(item);\r\n      } else {\r\n        this.basket.push({\r\n          name: item.name,\r\n          price: item.price,\r\n          quantity: 1,\r\n        });\r\n      }\r\n    },\r\n    incrQtn(item) {\r\n      item.quantity++;\r\n    },\r\n    decrQtn(item) {\r\n      item.quantity--;\r\n\r\n      if (item.quantity === 0) {\r\n        this.basket.splice(this.basket.indexOf(item), 1);\r\n      }\r\n    },\r\n    filterAll() {\r\n      this.filterValue = '';\r\n    },\r\n    filterPills() {\r\n      this.filterValue = 'pills';\r\n    },\r\n    filterCannabis() {\r\n      this.filterValue = 'cannabis';\r\n    },\r\n    filterCrystal() {\r\n      this.filterValue = 'crystal';\r\n    },\r\n    filterPowder() {\r\n      this.filterValue = 'powder';\r\n    },\r\n  },\r\n  computed: {\r\n    subTotal() {\r\n      var subCost = 0;\r\n      for (var items in this.basket) {\r\n        var individualItem = this.basket[items];\r\n        subCost += individualItem.quantity * individualItem.price;\r\n      }\r\n      return subCost;\r\n    },\r\n    total() {\r\n      var deliveryPrice = 0.0005;\r\n      var totalCost = this.subTotal;\r\n      return totalCost + deliveryPrice;\r\n    },\r\n    itemFilter() {\r\n      return this.menuItems.filter((filterI) =>\r\n        filterI.filter.includes(this.filterValue)\r\n      );\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n#rowCont {\r\n  display: flex;\r\n  justify-content: center;\r\n}\r\n\r\n#rowProd {\r\n  max-width: 80vw;\r\n  padding-top: 70px;\r\n}\r\n#products {\r\n  margin: 15px;\r\n  height: 376px;\r\n  width: 310px;\r\n  text-align: center;\r\n  span {\r\n    font-size: 1.6em;\r\n    font-weight: bold;\r\n  }\r\n  p {\r\n    position: absolute;\r\n    left: 5px;\r\n    bottom: 0;\r\n    margin: 25px 30px;\r\n  }\r\n  .v-btn {\r\n    position: absolute;\r\n    right: 5px;\r\n    bottom: 0;\r\n    margin: 20px;\r\n  }\r\n}\r\n#product_img {\r\n  width: 100%;\r\n  height: 65%;\r\n}\r\n\r\n#drug-table {\r\n  position: absolute;\r\n}\r\n\r\n.drug-filter {\r\n  background: rgb(17, 17, 17);\r\n  padding: 5px;\r\n  color: #fefefe;\r\n}\r\n\r\n.drug-filter-content {\r\n  a {\r\n    margin: 5px;\r\n    color: #fefefe !important;\r\n    transition-duration: 0.5s;\r\n    &:hover {\r\n      color: #ff6d00 !important;\r\n      transition-duration: 0.5s;\r\n    }\r\n  }\r\n}\r\n\r\n#drugBasket {\r\n  background-color: #fefefe;\r\n  height: fit-content;\r\n  border-radius: 7px;\r\n  margin-top: 85px;\r\n}\r\n</style>\r\n"]}]}